   JASS_BANNER_SENDMAIL             JASS_BANNER_SSHD
   JASS_BANNER_TELNETD              JASS_CHECK_MINIMIZED
   JASS_CORE_PATTERN                JASS_CPR_MGT_USER
   JASS_CRON_ALLOW                  JASS_CRON_DENY
   JASS_CRON_LOG_SIZE               JASS_CRYPT_ALGORITHMS_ALLOW
